/*
* Default styles for all the basic elements.
*/

div
{
    display: block;
}

p
{
    display: block;
}

h1
{
    display: block;
}

em
{
    font-style: italic;
}

strong
{
    font-weight: bold;
}

datagrid
{
    display: block;
}

select, dataselect, datacombo
{
    text-align: left;
}

tabset tabs
{
    display: block;
}

