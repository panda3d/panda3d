}

////////////////////////////////////////////////////////////////////
//     Function: IffChunk::get_id
//       Access: Public
//  Description: Returns the ID associated with this chunk.
////////////////////////////////////////////////////////////////////
INLINE IffId IffChunk::
get_id() const {
  return _id;
}

////////////////////////////////////////////////////////////////////
//     Function: IffChunk::set_id
//       Access: Public
//  Description: Changes the ID associated with this chunk.
////////////////////////////////////////////////////////////////////
INLINE void IffChunk::
set_id(IffId id) {
  _id = id;
}
