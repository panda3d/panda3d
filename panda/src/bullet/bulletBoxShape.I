//       Access: Published
//  Description:
////////////////////////////////////////////////////////////////////
INLINE BulletBoxShape::
BulletBoxShape(const BulletBoxShape &copy) :
  _shape(copy._shape) {
}

////////////////////////////////////////////////////////////////////
//     Function: BulletBoxShape::Copy Assignment Operator
//       Access: Published
//  Description:
////////////////////////////////////////////////////////////////////
INLINE void BulletBoxShape::
operator = (const BulletBoxShape &copy) {
  _shape = copy._shape;
}

