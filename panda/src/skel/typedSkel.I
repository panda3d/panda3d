 */
INLINE TypedSkel::
TypedSkel() {
}

/**
 *
 */
INLINE TypedSkel::
~TypedSkel() {
}

/**
 * Stores an integer value.
 */
INLINE void TypedSkel::
set_value(int n) {
  _value = n;
}

/**
 * Retreives a value that was previously stored.
 */
INLINE int TypedSkel::
get_value() {
  return _value;
}
